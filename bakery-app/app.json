{
  "expo": {
    "name": "Булочки Павлова",
    "slug": "bakery-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3D2314"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bakery.pavlova"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3D2314"
      },
      "package": "com.bakery.pavlova"
    },
    "web": {},
    "scheme": "bakery-app",
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
